// 미완성
#define _CRT_SECURE_NO_WARNINGS
#define MAX 100
#include <stdio.h>
#include <stdlib.h>

void main()
{
    int row, col, i, j, s[2], f[2];
    FILE* fp;
    if ((fp = fopen("maze1.txt", "r")) == NULL) {
        printf("Error");
        return 0;
    }
    fscanf(fp, "%d %d", &row, &col);

    int **matrix = (int*)malloc(sizeof(int) * (row+2));
    for (i = 0;i < row + 2;i++)
        matrix[i] = (int*)malloc(sizeof(int) * (col+2));

    for (i = 0;i < row+1;i++) {
        for (j = 0;j < col+1;j++) {
            matrix[i][j] = 1;
        }
    }

    printf("MAZE\n");
    for (i = 1;i <= row;i++) {
        for (j = 1;j <= col;j++) {
            fscanf(fp, "%d", &matrix[i][j]);
            printf("%3d", matrix[i][j]);
        }
        printf("\n");
    }
    printf("The path is:\n");

    fscanf(fp, "%d %d %d %d", &s[0], &s[1], &f[0], &f[1]);
    matrix[s[0]][s[1]] = 3;
    matrix[f[0]][f[1]] = 6;
    fclose(fp);






}
